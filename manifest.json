{
	"manifest_version": 2,
	"name": "一键复制csdn代码",
	"version": "1.0",
	"description": "无需登录一键复制csdn代码。支持选中代码，或者代码右上角按钮的一键复制。",
	"author": "openHacking",
	"homepage_url": "https://github.com/openHacking/copy-csdn",
	"icons":
	{
		"48": "icon.png",
		"128": "icon.png"
	},
	"browser_action": 
	{
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"content_scripts": 
	[
		{
			"matches": ["https://blog.csdn.net/*"],
			"js": ["content-script.js"],
			"css": ["css/custom.css"]
		}
	],
	"omnibox": { "keyword" : "copy" },
	"web_accessible_resources": 
	[
		"inject.js"
	]
}